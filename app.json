{
  "expo": {
    "name": "Cavaleiros da BÃ­blia",
    "slug": "cavaleiros-da-biblia",
    "version": "1.0.0",
    "scheme": "cavaleiros-da-biblia",
    "newArchEnabled": false,
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cavaleiros.biblia",
      "versionCode": 1,
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa717349-1c99-453b-9e68-971eac0c9b2c"
      }
    }
  }
}
